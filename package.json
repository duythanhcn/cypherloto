    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-config": "^0.12.0",
    "react-native-countdown-component": "^2.7.1",
    "react-native-date-picker": "^2.7.8",
    "react-native-device-info": "^5.5.3",
